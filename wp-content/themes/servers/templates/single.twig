{% extends "base_single.twig" %}


{% block content %}
	<div class="cont_bdy_midarea1">
		<div class="cont_bdy_mid">
			<div class="cont_bdy_midtop">
				<ul>
					<li><a class="selected" href="http://www.computerrecyclingsf.com/services/it-asset-disposition-sf-bay.html">ITAD <br>Overview</a></li>
					<li><a href="http://www.computerrecyclingsf.com/it-asset-disposition/itad-process-sf-bay.html">ITAD <br>Process</a></li>
					<li><a href="http://www.computerrecyclingsf.com/it-asset-disposition/it-asset-processing-sf-bay.html">IT Asset <br>Processing</a></li>
					<li><a href="http://www.computerrecyclingsf.com/it-asset-disposition/lease-returns-sf-bay.html">Lease <br>Returns</a></li>
					<li><a href="http://www.computerrecyclingsf.com/it-asset-disposition/evaluation-sf-bay.html">Evaluation <br>Request</a></li>
				</ul>
				<div class="spacer"></div>
				<select onchange="getval(this.value)"><option>ITAD Overview</option><option>ITAD Process</option><option>IT Asset Processing</option><option>Lease Returns</option><option>Evaluation Request</option></select></div>

			<div class="cont_bdy_midbtm">
				<div id="div1" class="cont_bdy_midbtm_area">
					<div class="overview1_con">
						{{ post.content|raw }}
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="content-wrapper">
		<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
			<section class="article-content">
				<h1 class="article-h1">{{post.title}}</h1>
				<p class="blog-author">
					<span>By</span><a href="{{post.author.path}}"> {{ post.author.name }} </a><span>&bull;</span> {{ post.post_date|date}}
				</p>
				<div class="article-body">

				</div>
			</section>
			<section class="comments">
				<div class="respond">
					<h3 class="h2">Comments</h3>
					{{ comment_form }}
				</div>
				<div class="responses">
					{% for cmt in post.get_comments() %}
						{% include "comment.twig" with {comment:cmt} %}
					{% endfor %}
				</div>
			</section>
		</article>
	</div><!-- /content-wrapper -->
{% endblock %}
